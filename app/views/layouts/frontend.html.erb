<!DOCTYPE html>
<html>
<head>
  <title>Trenti</title>
  <%= stylesheet_link_tag    'application', 'frontend', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'frontend', 'data-turbolinks-track' => true %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%= csrf_meta_tags %>
</head>
<body>
<div id="fb-root"></div>
<script>(function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/es_ES/sdk.js#xfbml=1&version=v2.5";
    fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
<%= render partial: 'frontend/header' %>
<%= yield %>
<%= render partial: 'frontend/footer' %>
<a href="#" class="scrollup"><span class="glyphicon glyphicon-arrow-up" aria-hidden="true"></span></a>
</body>
</html>
